@{
    ViewBag.Title = "Index";
}

@model Services.ViewModels.FriendsModels.FriendListViewModel

<div class="panel panel-default">
    <div class="panel-body">
        <h3>Friends</h3>
    </div>
    <div class="panel-footer">
        @foreach (var friend in @Model.Friends)
        {
            <div>
                @friend.Name
            </div>
        }
    </div>
</div>

